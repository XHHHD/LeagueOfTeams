<Page x:Class="LeagueOfTeamsUI.Views.Pages.Menu.SmollServicesLinks.FirstTopTeamLogo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LeagueOfTeamsUI.Views.Pages.Menu.SmollServicesLinks"
      mc:Ignorable="d" 
      d:DesignHeight="230" d:DesignWidth="378"
      Title="FirstTopTeamLogo">

    <Button Height="230" Width="378" Click="TeamButton_Click" Background="{x:Null}" BorderBrush="{x:Null}">
        <StackPanel Orientation="Vertical">
            <Image x:Name="TeamImg" Height="140" Width="140" Margin="3"
                   Source="/Resources/Default/icons8-ос-free-bsd-100-white.png"/>
            <TextBlock x:Name="TeamName" FontFamily="Castellar" FontSize="48" Text="Team Name"/>
        </StackPanel>
    </Button>
</Page>
