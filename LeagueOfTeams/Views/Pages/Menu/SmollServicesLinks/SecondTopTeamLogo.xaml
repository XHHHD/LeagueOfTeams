<Page x:Class="LeagueOfTeamsUI.Views.Pages.Menu.SmollServicesLinks.SecondTopTeamLogo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LeagueOfTeamsUI.Views.Pages.Menu.SmollServicesLinks"
      mc:Ignorable="d" 
      d:DesignHeight="180" d:DesignWidth="288"
      Title="SecondTopTeamLogo">

    <Button Height="160" Width="268" Click="TeamButton_Click" Background="{x:Null}" BorderBrush="{x:Null}">
        <StackPanel Orientation="Vertical">
            <Border Background="Black" Height="100" Width="100" Margin="0,10,0,0">
                <Image x:Name="TeamRank" Margin="3"/>
            </Border>
            <TextBlock x:Name="TeamName" FontFamily="Castellar" FontSize="36" Text="Team Name"/>
        </StackPanel>
    </Button>
</Page>
