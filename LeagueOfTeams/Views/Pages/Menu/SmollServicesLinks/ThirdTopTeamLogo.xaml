<Page x:Class="LeagueOfTeamsUI.Views.Pages.Menu.SmollServicesLinks.ThirdTopTeamLogo"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LeagueOfTeamsUI.Views.Pages.Menu.SmollServicesLinks"
      mc:Ignorable="d" 
      d:DesignHeight="160" d:DesignWidth="308"
      Title="ThirdTopTeamLogo">

    <Button Height="140" Width="268" Click="TeamButton_Click" Background="{x:Null}" BorderBrush="{x:Null}">
        <StackPanel Orientation="Vertical">
            <Image x:Name="TeamRank" Height="80" Width="80" Margin="0,10,0,0"
                   Source="/Resources/Default/icons8-ос-free-bsd-100-white.png"/>
            <TextBlock x:Name="TeamName" FontFamily="Castellar" FontSize="32" Text="Team Name"/>
        </StackPanel>
    </Button>
</Page>
